{
  "name": "gemini-nano-banana",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:fast": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev --turbo",
    "dev:legacy": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "clean": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "perf": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev --turbo"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.9.2",
    "@stripe/stripe-js": "^2.4.0",
    "@types/nodemailer": "^7.0.1",
    "next": "14.2.5",
    "nodemailer": "^7.0.6",
    "react": "^18",
    "react-dom": "^18",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/stripe": "^8.0.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5"
  }
}
